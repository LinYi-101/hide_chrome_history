{
  "manifest_version": 3,
  "name": "__MSG_ext_name__",
  "version": "1.0",
  "description": "Automatically clears browsing history using keywords or regular expressions.",
  "permissions": [
    "history",
    "alarms",
    "storage"
  ],
  "default_locale": "en",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}